import xlsxwriter

class ExcelToneLog:
    workbook = None
    worksheet = None

    def __init__(self, f):
        #create file excel name
        excelpath = f + "\ToneAnalysis" + ".xlsx"

        #create a workbook and sheet
        self.workbook = xlsxwriter.Workbook(excelpath)
        self.worksheet = self.workbook.add_worksheet()

        self.headers()

    def headers(self):
        self.worksheet.write(0, 0, 'File Name')
        self.worksheet.write(0, 1, 'Tone 1 Time Stamp')
        self.worksheet.write(0, 2, 'Frequency (1)')
        self.worksheet.write(0, 3, 'Peak (1)')
        self.worksheet.write(0, 4, 'RMS (1)')
        self.worksheet.write(0, 5, 'Frequency (2)')
        self.worksheet.write(0, 6, 'Peak (2)')
        self.worksheet.write(0, 7, 'RMS (2)')
        self.worksheet.write(0, 8, 'Phase Shift')
        self.worksheet.write(0, 9, 'Tone 2 Time Stamp')
        self.worksheet.write(0, 10, 'Frequency (1)')
        self.worksheet.write(0, 11, 'Peak (1)')
        self.worksheet.write(0, 12, 'RMS (1)')
        self.worksheet.write(0, 13, 'Frequency (2)')
        self.worksheet.write(0, 14, 'Peak (2)')
        self.worksheet.write(0, 15, 'RMS (2)')
        self.worksheet.write(0, 16, 'Phase Shift')
